class muscle:
    def __init__(self, origin, insertion, joint_action):
        self.origin = origin
        self.insertion = insertion
        self.joint_action = joint_action

pectoralis_major_clavicular = muscle("clavicle medial half", "humerus crest of greater tubercle", "shoulder horizontal adduction, shoulder medial rotation, shoulder flexion")
